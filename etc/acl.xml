<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:Acl/etc/acl.xsd">
    <acl>
        <resources>
            <resource id="Magento_Backend::admin">
                <resource id="Magento_Backend::stores">
                    <resource id="Magento_Backend::stores_settings">
                        <resource id="Magento_Config::config">
                            <resource id="SqualoMail_SqmMcMagentoTwo::config_sqmmc" title="SqualoMail section" sortOrder="60" />
                        </resource>
                    </resource>
                </resource>
                <resource id="SqualoMail_SqmMcMagentoTwo::main_menu" title="SqualoMail">
                    <resource id="SqualoMail_SqmMcMagentoTwo::error_grid" title="SqualoMail Error Grid" sortOrder="100"/>
                    <resource id="SqualoMail_SqmMcMagentoTwo::stores_grid" title="SqualoMail Stores Grid" sortOrder="110">
                        <resource id="SqualoMail_SqmMcMagentoTwo::stores_edit" title="SqualoMail Stores Edit" sortOrder="120"/>
                    </resource>
                </resource>
            </resource>
        </resources>
    </acl>
</config>